{
	"work_queue": {
		"name": "active-data-etl-beta",
		"debug": true,
		"$ref": "file://~/private.json#aws_credentials"
	},
	"param": {
		"threads": 1,
		"wait_forever": true,
		"keep_unknown_on_queue": true
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData",
			"User-Agent": "testlog-etl"
		},
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 10000000
	},
	"hg": {
		"use_cache": true,
		"hg":{
			"url": "https://hg.mozilla.org"
		},
		"branches": {
			"host": "http://192.168.2.98",
			"port": 9200,
			"index": "branches",
			"type": "branch",
			"timeout": 300,
			"schema": {
				"$ref": "//../../schema/branch.json"
			},
			"debug": false,
			"limit_replicas": false,
			"use_cache": true
		},
		"repo": {
			"host": "http://localhost",
			"port": 9200,
			"index": "repo",
			"type": "revision",
			"timeout": 300,
			"schema": {
				"$ref": "//../../schema/revision.json"
			},
			"debug": false,
			"limit_replicas": false
		}
	},
	"workers": [
		{
			"name": "pulse2es",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-pulse-beta",
				"key_format": "a:b"
			},
			"destination": {
				"host": "http://192.168.2.98",
				"port": 9200,
				"index": "pulse",
				"type": "pulse_log",
				"timeout": 300,
				"schema": {
					"$ref": "//../../schema/pulse_logs.json"
				},
				"debug": true,
				"limit_replicas": true
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_es.process",
			"type": "join"
		},
		{
            "name": "pulse_tc_to_codecoverage",
            "source": {
                "$ref": "file://~/private.json#aws_credentials",
                "bucket": "active-data-task-cluster-logger-beta",
                "key_format": "a:b"
            },
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-codecoverage-beta",
				"public": true,
				"key_format": "a:b.c"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_tc_logs.process",
            "type": "join"
        },
