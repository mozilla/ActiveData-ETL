{
	"work_queue": {
		"name": "active-data-etl-beta",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"param": {
		"threads": 4,
		"wait_forever": true,
		"keep_unknown_on_queue": false
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 10000000,
		"testlog_etl.transforms.pulse_block_to_test_result_logs.PARSE_TRY": true
	},
	"hg": {
		"use_cache": true,
		"hg":{
			"url": "https://hg.mozilla.org"
		},
		"branches": {
			"host": "http://localhost",
			"port": 9200,
			"index": "branches",
			"type": "branch",
			"timeout": 300,
			"schema": {
				"$ref": "//../../schema/branch.json"
			},
			"debug": false,
			"limit_replicas": false
		},
		"repo": {
			"host": "http://localhost",
			"port": 9200,
			"index": "repo",
			"type": "revision",
			"timeout": 300,
			"schema": {
				"$ref": "//../../schema/revision.json"
			},
			"debug": false,
			"limit_replicas": false
		}
	},
	"workers": [
		{
			"name": "pulse2es",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-pulse-beta",
				"key_format": "a:b"
			},
			"destination": {
				"host": "http://localhost",
				"port": 9200,
				"index": "pulse",
				"type": "pulse_log",
				"timeout": 300,
				"schema": {
					"$ref": "//../../schema/pulse_logs.json"
				},
				"debug": false,
				"limit_replicas": true
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_es.process",
			"type": "join"
		},
		{
			"name": "pulse2perfherder",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-pulse-beta",
				"public": true,
				"key_format": "a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-perfherder-beta",
				"public": true,
				"key_format": "a:b.c"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_perfherder_logs.process",
			"type": "join",
			"transform_type": "bulk"
		},
		{
			"name": "bbb2jobs",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-buildbot-beta",
				"key_format": "a.b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-jobs-beta",
				"public": true,
				"key_format": "a.b"
			},
			"notify": {
				"name": "active-data-index-jobs-beta",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "testlog_etl.transforms.buildbot_block_to_job_logs.process",
			"type": "join",
			"transform_type": "bulk"
		},
		{
			"name": "talos2perf",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-talos-beta",
				"key_format": "a:b.c"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-perf-beta",
				"public": true,
				"key_format": "a:b.c"
			},
			"notify": {
				"name": "active-data-index-perf-beta",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transform_type": "bulk",
			"transformer": "testlog_etl.transforms.talos_log_to_perf_logs.process",
			"type": "join"
		},
		{
			"name": "pulse2test_result",
			"source": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-pulse-beta",
				"key_format": "a:b"
			},
			"destination": {
				"$ref": "file://~/private.json#aws_credentials",
				"bucket": "active-data-test-result-beta",
				"public": true,
				"key_format": "a:b.c"
			},
			"notify": {
				"name": "active-data-index-beta",
				"$ref": "file://~/private.json#aws_credentials"
			},
			"transformer": "testlog_etl.transforms.pulse_block_to_test_result_logs.process",
			"type": "join"
		}
	],
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][Beta] Problem in ETL",
				"$ref": "file://~/private.json#email"
			}
		]
	}
}
